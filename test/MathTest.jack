class MathTest {
    function void main() {
        do testMult();
        return;
    }

    function void testMult() {
        do Output.println();
        do Output.printString("---------------");
        do Output.println();
        do Output.printString("Test multiply");
        do Output.println();
        do Output.println();

        do Output.printString("Basic test");
        do Output.println();
        do Output.printString("Expected 8, got: ");
        do Output.printInt(Math.multiply(2,4));
        do Output.println();
        do Output.println();

        do Output.printString("Negative test");
        do Output.println();
        do Output.printString("Expected -8, got: ");
        do Output.printInt(Math.multiply(-2,4));
        do Output.println();
        do Output.println();

        do Output.printString("Double negative test");
        do Output.println();
        do Output.printString("Expected 8, got: ");
        do Output.printInt(Math.multiply(-2, -4));
        do Output.println();

        return;
    }
}
